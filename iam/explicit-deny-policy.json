{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "DenyAccessOutsideUserUploads",
            "Effect": "Deny",
            "Action": "s3:*",
            "Resource": "*",
            "Condition": {
                "StringNotLike": {
                    "s3:prefix": "user-uploads/*"
                }
            }
        }
    ]
}